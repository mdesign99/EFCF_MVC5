@ARTICLE{Celik2024332,
	author = {Celik, Pelin},
	title = {Haptic Recording Experience: The Iceberg Model as a Serious Game for Decision Making in Systemic Design Oriented Leadership (SDOL)},
	year = {2024},
	journal = {Lecture Notes in Computer Science (including subseries Lecture Notes in Artificial Intelligence and Lecture Notes in Bioinformatics)},
	volume = {14475 LNCS},
	pages = {332 – 341},
